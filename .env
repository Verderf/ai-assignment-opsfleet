# Google Gemini API Key
GOOGLE_API_KEY= PUT YOUR_GEMINI_API_KEY_HERE 

# Gemini Model (default: gemini-pro, alternatives: gemini-1.5-pro, gemini-1.5-flash)
GEMINI_MODEL=gemini-2.5-flash

# Path to Google Cloud Service Account Key (for BigQuery)
# Inside Docker, this maps to /app/credentials/gcp-key.json
GOOGLE_APPLICATION_CREDENTIALS=./credentials/gcp-key.json

# Optional: Project ID for BigQuery
GOOGLE_CLOUD_PROJECT=ai-assignment-1-486023
